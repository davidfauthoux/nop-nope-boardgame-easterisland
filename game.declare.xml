	<item>
		<content>
			<span>switchaudio</span>
		</content>
		<trigger>drop:audio</trigger>
		<script>
		<![CDATA[
			if (!here.live()) {
				//%% here.move("video", 1, here); // To be able to mute/unmute
				return;
			}
			if (hover.live()) {
				here.destroy("video", 1);
			} else {
				here.drop("video", 1);
			}
		]]>
