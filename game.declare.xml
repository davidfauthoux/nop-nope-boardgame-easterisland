	<item>
		<kind>breakwhale</kind>
		<content>
			<span>instant:BreakWhale</span>
		</content>
		<trigger>instant</trigger>
		<script>
		<![CDATA[
			console.log("*instant:BreakWhale");
			console.log(here.location);
			var whales = here.count("whale");
			if (whales > 0) { // Prevents to destroy infinite whales
				var fishes = whales * 5;
				console.log("Breaking " + whales + " whales into " + fishes + " fishes");
				here.drop("fish", fishes);
				here.destroy("whale");
			}
		]]>
		</script>
		<property>
			<key>hidden</key>
		</property>
	</item>

