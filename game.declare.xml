	<item>
		<trigger>key:a</trigger>
		<trigger>key:Meta+b</trigger>
		<script>
		<![CDATA[
			window.alert("Hovered " + hover.kind + " in " + here.location + " [" + (hover.live() ? "live" : "-") + "]");
		]]>
		</script>
	</item>

	<item>
		<content>
			<span>Take</span>
		</content>
		<trigger>key:t</trigger>
		<script>
		<![CDATA[
			console.log("*Take");
			if (hover.kind !== undefined) {
				var took = false;
				here.items().each(function(item) {
					var player = item.is("meeple");
					if (player !== undefined) {
						log("The player " + player + " took a " + hover.kind);
						here.move(hover.kind, 1, pools.get("bank-" + player));
						took = true;
					}
				});
				if (!took) {
					log("No player here");
				}
			}
		]]>
		</script>
	</item>

	<item>
		<content>
			<span>LiveTake</span>
		</content>
		<trigger>key:Space</trigger>
		<script>
		<![CDATA[
			each(pools, function(pool, player) {
				if (pool.live()) {
					pool.drop(hover.kind, 1);
					here.destroy(hover.kind, 1);
				}
			});
		]]>
		</script>
	</item>

